{
  "name": "aedart/testing-gst",
  "description": "Testing utility for custom getter setter traits",
  "keywords": [
    "Testing",
    "Getter",
    "Setter",
    "Utility"
  ],
  "homepage": "https://github.com/aedart/testing-gst",
  "license": "BSD-3-Clause",
  "type": "library",
  "authors": [
    {
      "name": "Alin Eugen Deac",
      "email": "aedart@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "phpunit/phpunit": "~5.3"
  },
  "require-dev": {
    "aedart/license": "1.*",
    "aedart/license-file-manager": "~1.6",
    "codeception/codeception": "~2.1"
  },
  "autoload": {
    "psr-4": {
      "Aedart\\Testing\\GST\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "scripts": {
    "post-update-cmd": "vendor/bin/license-manager license:copy vendor/aedart/license/aedart/BSD-3-Clause"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
